# We use a little hack so that the snippet is expanded and parsed correctly.
snippet s "Ultisnips snippet definition" b
`!p snip.rv = "snippet"` ${1:Tab_trigger} "${2:Description}" ${3}
${0:${VISUAL}}
`!p snip.rv = "endsnippet"`
endsnippet

snippet g "Ultisnips global definition" b
global !p
$0
endglobal
endsnippet
